{
    "layout":
    {
        "fig_width": 8,
        "top_margin": 0,
        "left_margin": 0,
        "right_margin": 0,
        "panel_height": 3,
        "grid_wspace": 100
    },
    "x_display":{
        "global_x_field": "time",
        "label": "Time (s)",
        "global_x_ticks": [3.5,4.5]
    },
    "formatting":
    {
        "x_label_fontsize": 14,
        "y_label_fontsize": 14,
        "tick_fontsize": 12,
        "legend_fontsize": 10,
        "y_label_pad": 50,
        "legend_bbox_to_anchor": [1.0, 1.05],
        "palette": "muted",
        "patch_alpha": 0.5
    },
    "processing":
    {
        "envelope_n": 200
    },
    
    "panels":
    [
       
        {
            "column": 1,
            "y_info":
            {
                "label":"Ventricular\npressure\n(mmHg)",
                "series":
                [
                    {
                        "field": "pressure_avg",
                        "style": "line",
                        "field_label": "Average"
                    },
                    {
                        "field": "pressure_pos_std",
                        "style": "line",
                        "field_label": "Average + std"
                    },
                    {
                        "field": "pressure_neg_std",
                        "style": "line",
                        "field_label": "Average - std"
                    }
                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label":"Ventricular\nvolume\n(liters)",
                "series":
                [
                    {
                        "field": "volume_avg",
                        "style": "line",
                        "field_label": "Average"
                    },
                    {
                        "field": "volume_pos_std",
                        "style": "line",
                        "field_label": "Average + std"
                    },
                    {
                        "field": "volume_neg_std",
                        "style": "line",
                        "field_label": "Average - std"
                    }
                ]
            }
        }
    ]
}
